{
    "name": "Getting Started Example",
    "description": "It doesn't like people. If you show yourself, then the browser will remove itself immediately",
    "version": "1.1",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["storage"],
    "options_page":"options.html",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
         
          "128": "/images/ShyCon.png"
        }
      },
      "icons": {
        
        "128": "/images/ShyCon.png"
      }
  }
